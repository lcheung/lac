package app.demo.pong{	import flash.display.Sprite;		public class Net extends Sprite	{		function setup(height:Number, strokeLength:Number, gapLength:Number)		{			var strokeAndGapLength:Number = strokeLength + gapLength;			var numStrokes:Number = height / strokeAndGapLength;			var curY:Number = 0;			this.graphics.lineStyle(2, 0xFFFFFF, 1.0);			for (var i:Number = 0; i < numStrokes; ++i)			{				this.graphics.lineTo(0, curY + strokeLength);				curY += strokeAndGapLength;				this.graphics.moveTo(0, curY);			}		}	}}